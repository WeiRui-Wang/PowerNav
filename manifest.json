{
  "manifest_version": 3,
  "name": "PowerNav",
  "version": "1.0.1",
  "description": "Facilitate navigation with keyboard for accessibility user and power user",
  "permissions": ["activeTab", "scripting"],
  "icons": {
    "128": "default_icon.png"
  },
  "action": {
    "default_title": "PowerNav",
    "default_icon": "default_icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content_scripts.js"]
    }
  ]
}
