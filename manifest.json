{
  "manifest_version": 3,
  "name": "PowerNav",
  "version": "0.2.0",
  "description": "Facilitates navigation for accessibility user and poweruser",
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content_scripts.js"
      ]
    }
  ]
}