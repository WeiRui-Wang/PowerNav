{
  "manifest_version": 3,
  "name": "PowerNav",
  "version": "0.1.0",
  "description": "Facilitates navigation for accessibility and power user",
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["content_scripts.js"]
    }
  ]
}